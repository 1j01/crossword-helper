	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python Debugger: gen-puzzle",
			"type": "debugpy",
			"request": "launch",
			"module": "src.cli",
			"args": [
				"gen-puzzle",
				"--max-words", "1000",
				"--max-placement-attempts", "10000",
				"--letters-per-cell", "2",
				"--format", "ascii"
			],
			"console": "integratedTerminal",
		}
	]
}